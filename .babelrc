{
  "presets": [
    "env",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-export-extensions",
    ["module-resolver", {
      "alias": {
        "adapters": "./bin/adapters",
        "config": "./bin/config",
        "core": "./bin/core",
        "api": "./bin/api",
        "modules": "./bin/modules"
      }
    }]
  ]
}
